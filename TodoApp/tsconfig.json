{
  "compilerOptions": {
    "outDir": "./dist/",
    "jsx": "react-jsx",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "module": "es6",
    "target": "es6",
    "allowJs": false,
    "moduleResolution": "node",
    "plugins": [{ "name": "typescript-plugin-css-modules" }]
  }
}
